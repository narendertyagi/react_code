    "wpack:dev:watch": "NODE_ENV=development yarn wpack_dev -w & yarn wpack_css -w",
    "build1": "yarn wpack_prod & yarn wpack_css",
    "symlink_bower": "node -e \"try { require('fs').symlinkSync(require('path').resolve('node_modules/@bower_components'), 'public/bower', 'junction') } catch (e) { }\""
